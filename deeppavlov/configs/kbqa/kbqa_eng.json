{
  "dataset_reader": {
    "class_name": "kbqa_reader",
    "data_path": "{DOWNLOADS_PATH}/wikidata_eng/"
  },
  "dataset_iterator": {
    "class_name": "data_learning_iterator"
  },
  "chainer": {
    "in": ["x"],
    "in_y": ["y"],
    "pipe": [
      {
        "id": "classes_vocab",
        "class_name": "simple_vocab",
        "fit_on": [
          "y"
        ],
        "save_path": "{MODEL_PATH}/relation_prediction/classes.dict",
        "load_path": "{MODEL_PATH}/relation_prediction/classes.dict",
        "in": "y",
        "out": "y_ids"
      },
      {
        "in": [
          "x"
        ],
        "out": [
          "x_prep"
        ],
        "class_name": "dirty_comments_preprocessor"
      },
      {
        "in": "x_prep",
        "out": "x_tok",
        "id": "my_tokenizer",
        "class_name": "nltk_tokenizer",
        "tokenizer": "wordpunct_tokenize"
      },
      {
        "in": "x_tok",
        "out": "x_emb",
        "id": "my_embedder",
        "class_name": "fasttext",
        "load_path": "{DOWNLOADS_PATH}/embeddings/wordpunct_tok_reddit_comments_2017_11_300.bin",
        "pad_zero": true
      },
      {
        "in": "y_ids",
        "out": "y_onehot",
        "class_name": "one_hotter",
        "depth": "#classes_vocab.len",
        "single_vector": true
      },
      {
        "in": [
          "x_emb"
        ],
        "in_y": [
          "y_onehot"
        ],
        "out": [
          "y_pred_probas"
        ],
        "main": true,
        "class_name": "keras_classification_model",
        "save_path": "{MODEL_PATH}/relation_prediction/model",
        "load_path": "{MODEL_PATH}/relation_prediction/model",
        "embedding_size": "#my_embedder.dim",
        "n_classes": "#classes_vocab.len",
        "kernel_sizes_cnn": [
          3,
          5,
          7
        ],
        "filters_cnn": 256,
        "optimizer": "Adam",
        "learning_rate": 0.01,
        "learning_rate_decay": 0.1,
        "loss": "binary_crossentropy",
        "last_layer_activation": "softmax",
        "coef_reg_cnn": 1e-3,
        "coef_reg_den": 1e-2,
        "dropout_rate": 0.5,
        "dense_size": 100,
        "model_name": "cnn_model"
      },
      {
        "in": "y_pred_probas",
        "out": "y_pred_ids",
        "class_name": "proba2labels",
        "max_proba": true
      },
      {
        "in": ["x"],
        "class_name": "lazy_tokenizer",
        "out": ["x_tokens"]
      },
      {
        "in": ["x_tokens"],
        "class_name": "str_lower",
        "out": ["x_lower"]
      },
      {
        "in": ["x_lower"],
        "class_name": "sanitizer",
        "nums": true,
        "out": ["x_san"]
      },
      {
        "in": ["y"],
        "id": "tag_vocab",
        "class_name": "simple_vocab",
        "pad_with_zeros": true,
        "fit_on": ["y"],
        "save_path": "{MODEL_PATH}/ner_kb_eng/tag.dict",
        "load_path": "{MODEL_PATH}/ner_kb_eng/tag.dict",
        "out": ["y_ind"]
      },
      {
        "in": ["x_tokens"],
        "class_name": "char_splitter",
        "out": ["x_char"]
      },
      {
        "in": ["x_char"],
        "id": "char_vocab",
        "class_name": "simple_vocab",
        "pad_with_zeros": true,
        "fit_on": ["x_char"],
        "save_path": "{MODEL_PATH}/ner_kb_eng/char.dict",
        "load_path": "{MODEL_PATH}/ner_kb_eng/char.dict",
        "out": ["x_char_ind"]
      },
      {
        "in": ["x_tokens"],
        "class_name": "mask",
        "out": ["mask"]
      },
      {
        "in": ["x_san"],
        "id": "glove_emb",
        "class_name": "glove",
        "pad_zero": true,
        "load_path": "{DOWNLOADS_PATH}/embeddings/glove.6B.100d.txt",

        "out": ["x_emb"]
      },
      {
        "id": "embeddings_char",
        "class_name": "emb_mat_assembler",
        "character_level": true,
        "emb_dim": 32,
        "embedder": "#glove_emb",
        "vocab": "#char_vocab"
      },
      {
        "id": "capitalization",
        "class_name": "capitalization_featurizer",
        "in": ["x_tokens"],
        "out": ["cap"]
      },
      {
        "in": ["x_emb", "mask", "x_char_ind", "cap"],
        "in_y": ["y_ind"],
        "out": ["y_predicted"],
        "class_name": "ner",
        "main": true,
        "token_emb_dim": "#glove_emb.dim",
        "n_hidden_list": [256, 256, 256],
        "net_type": "rnn",
        "cell_type": "lstm",
        "use_cudnn_rnn": true,
        "n_tags": "#tag_vocab.len",
        "capitalization_dim": "#capitalization.dim",
        "char_emb_dim": "#embeddings_char.dim",
        "save_path": "{MODEL_PATH}/ner_kb_eng/model",
        "load_path": "{MODEL_PATH}/ner_kb_eng/model",
        "char_emb_mat": "#embeddings_char.emb_mat",
        "two_dense_on_top": true,
        "use_crf": true,
        "use_batch_norm": true,
        "embeddings_dropout": true,
        "top_dropout": true,
        "intra_layer_dropout": false,
        "l2_reg": 0,
        "learning_rate": 3e-3,
        "learning_rate_drop_patience": 3,
        "dropout_keep_prob": 0.7
      },
      {
        "class_name": "entity_linker",
        "id": "linker",
        "load_path": "{DOWNLOADS_PATH}/wikidata_eng",
        "wiki_filename": "wiki_eng.pickle",
        "entities_filename": "wiki_eng_name_to_q_lowercase.pickle",
        "id_to_name_file": "wiki_eng_q_to_name.pickle",
        "inverted_index_filename": "inverted_index.pickle",
        "use_inverted_index": false,
        "rule_filter_entities": false,
        "debug": true
      },
      {
        "class_name": "template_matcher",
        "id": "template_m",
        "load_path": "{DOWNLOADS_PATH}/wikidata_eng/wiki_eng_q_to_name.pickle",
        "templates_filename": "templates_eng.pickle"
      },
      {
        "in": ["x_tokens", "y_predicted", "y_pred_probas"],
        "out": ["answers"],
        "load_path": "{DOWNLOADS_PATH}/wikidata_eng/wiki_eng_q_to_name.pickle",
        "class_name": "kb_answer_parser_wikidata",
        "classes_vocab_keys": "#classes_vocab.keys()",
        "top_k_classes": 5,
        "relations_maping_filename": "wiki_properties_id_to_name.pickle",
        "templates_filename": "templates_eng.pickle",
        "debug": true,
        "return_confidences": false,
        "lemmatize": false,
        "template_matcher": "#template_m",
        "linker": "#linker",
        "lang": "eng"
      }
    ],
    "out": ["answers"]
  },
  "train":{
    "metrics": [
      {
        "name": "kbqa_accuracy",
        "inputs": ["y", "answers"]
      }
    ]
  },
  "metadata": {
    "variables": {
      "ROOT_PATH": "~/.deeppavlov",
      "DOWNLOADS_PATH": "{ROOT_PATH}/downloads",
      "MODELS_PATH": "{ROOT_PATH}/models",
      "MODEL_PATH": "{MODELS_PATH}/kbqa_eng"
    },
    "requirements": [
      "{DEEPPAVLOV_PATH}/requirements/tf.txt",
      "{DEEPPAVLOV_PATH}/requirements/fasttext.txt"
    ],
    "download": [
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/wiki_eng.pickle",
        "subdir": "{DOWNLOADS_PATH}/wikidata_eng"
      },
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/wiki_eng_name_to_q_lowercase.pickle",
        "subdir": "{DOWNLOADS_PATH}/wikidata_eng"
      },
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/wiki_eng_q_to_name.pickle",
        "subdir": "{DOWNLOADS_PATH}/wikidata_eng"
      },
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/templates_eng.pickle",
        "subdir": "{DOWNLOADS_PATH}/wikidata_eng"
      },
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/wiki_properties_id_to_name.pickle",
        "subdir": "{DOWNLOADS_PATH}/wikidata_eng"
      },
      {
        "url": "http://files.deeppavlov.ai/embeddings/reddit_fastText/wordpunct_tok_reddit_comments_2017_11_300.bin",
        "subdir": "{DOWNLOADS_PATH}/embeddings"
      },
      {
        "url": "http://files.deeppavlov.ai/deeppavlov_data/kbqa_eng.tar.gz",
        "subdir": "{MODELS_PATH}/kbqa_eng"
      },
      {
        "url": "http://files.deeppavlov.ai/embeddings/glove.6B.100d.txt",
        "subdir": "{DOWNLOADS_PATH}/embeddings"
      }
    ]
  }
}
